#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x563018f600f0 .scope module, "tb" "tb" 2 3;
 .timescale -9 -12;
v0x563018fa61f0_0 .var "clk", 0 0;
v0x563018fa62c0_0 .var "reset", 0 0;
S_0x563018f60270 .scope module, "cnt" "counter" 2 8, 3 3 0, S_0x563018f600f0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "reset"
v0x563018f966c0_0 .net "clk", 0 0, v0x563018fa61f0_0;  1 drivers
v0x563018fa6010_0 .var "cnt", 15 0;
v0x563018fa60f0_0 .net "reset", 0 0, v0x563018fa62c0_0;  1 drivers
E_0x563018f96d90/0 .event negedge, v0x563018fa60f0_0;
E_0x563018f96d90/1 .event posedge, v0x563018f966c0_0;
E_0x563018f96d90 .event/or E_0x563018f96d90/0, E_0x563018f96d90/1;
    .scope S_0x563018f60270;
T_0 ;
    %wait E_0x563018f96d90;
    %load/vec4 v0x563018fa60f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x563018fa6010_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563018fa6010_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x563018fa6010_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563018f600f0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563018fa61f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563018fa62c0_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563018fa62c0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x563018f600f0;
T_2 ;
    %vpi_call 2 21 "$dumpfile", "test_out.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars" {0 0 0};
    %delay 1000000, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x563018f600f0;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x563018fa61f0_0;
    %inv;
    %assign/vec4 v0x563018fa61f0_0, 0;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb.v";
    "counter.v";
